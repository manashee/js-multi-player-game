{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "build/index.js",
  "engines": {
    "node": ">= 20.9.0"
  },
  "scripts": {
    "start": "tsx watch src/index.ts",
    "loadtest": "tsx loadtest/example.ts --room my_room --numClients 2",
    "build": "npm run clean && tsc",
    "clean": "rimraf build",
    "test": "mocha -r tsx test/**_test.ts --exit --timeout 15000",
    "client:text": "tsx src/client/views/text/app-text.ts",
    "client:phaser": "vite --config phaser.vite.config.ts",
    "client:babylon": "vite --config babylon.vite.config.ts",
    "build:phaser": "vite build --config phaser.vite.config.ts",
    "build:babylon": "vite build --config babylon.vite.config.ts",
    "build:all": "npm run build && npm run build:phaser && npm run build:babylon",
    "dev:all": "npm run build:all && concurrently -n TEXT,PHASER,BABYLON -c cyan,green,magenta \"PLAYER_NAME=p tsx src/client/views/text/app-text.ts\" \"vite --config phaser.vite.config.ts\" \"vite --config babylon.vite.config.ts\"",
    "dev:all:mac": "bash scripts/dev-all-mac.sh"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "@colyseus/loadtest": "^0.16.0",
    "@colyseus/testing": "^0.16.0",
    "@types/express": "^4.17.1",
    "@types/mocha": "^10.0.1",
    "concurrently": "^9.0.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.10.2",
    "typescript": "^5.0.4",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "@babylonjs/loaders": "^6.49.0",
    "@colyseus/monitor": "^0.16.0",
    "@colyseus/playground": "^0.16.0",
    "@colyseus/tools": "^0.16.0",
    "babylonjs": "^6.48.0",
    "colyseus": "^0.16.0",
    "colyseus.js": "^0.16.0",
    "express": "^4.18.2",
    "phaser": "^3.70.0"
  }
}
